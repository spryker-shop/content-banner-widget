{% extends model('component') %}

{% define config = {
    name: 'banner',
} %}

{% define modifiers = modifiers is not empty > 0 ? modifiers : ['default'] %}

{% block body %}
    {% for modifier in modifiers %}
        {% include molecule('banner-' ~ modifier, 'ContentBannerWidget') with {data: data} only %}
    {% endfor %}
{% endblock %}